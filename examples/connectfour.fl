func Game() {
  this.board = array 6, 7 default "empty";

  print(this.board);
}

Game.prototype.lowestFreeSlot = func (x, y) {
  print(this);

  if (this.board[x][y] !== "empty") {
    return [x, y - 1];
  } else {
    return this.lowestFreeSlot(x, y + 1);
  }
};

let x = new Game();
print(x.lowestFreeSlot(0, 0));
